type MemberInfo {
    name: String
    email: String
    job: String
    category: Category
    birthday: Date
    gender: String
}

type DiaryInfo {
    createdAt: Date!
    date: Date!
    feeling: Feeling!
    achievement: [Achievement]!
}
type Achievement {
    goal: Goal
    achieved: Boolean
}

type ChallengeInfo {
    startDate: Date!
    endDate: Date!
    participants: Int!
    maxParticipants: Int!
    category: Category!
    status: ChallengeStatus!
}