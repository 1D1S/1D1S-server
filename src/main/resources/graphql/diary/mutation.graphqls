extend type Mutation {
    createDiary(input: CreateDiaryInput!): Diary! # clear - 응답 잘 돌아옴
    updateDiary(DiaryId: ID!, input: CreateDiaryInput): Diary! # clear - 잘 수정 응답옴
    deleteDiary(DiaryId: ID!): Boolean # clear - 응답옴

    addDiaryLike(diaryId: ID!, memberId: ID!): Int! #clear
    cancelDiaryLike(diaryId: ID!, memberId: ID!): Int! # clear

    reportDiary(input: CreateDiaryReportInput!): Boolean! # clear
}

input CreateDiaryInput {
   # challengeId: ID#!
    title: String!
    content: String!
    feeling: Feeling!
    isPublic: Boolean
    #achievedGoalIds: [ID!]#!
    achievedDate: DateInput
    images : [String!]
}

input CreateDiaryReportInput{
    diaryId: ID!
    content: String!
    reportType: ReportType!
}