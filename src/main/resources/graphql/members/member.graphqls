type Member {
    id: ID!
    option: MemberOption! # 공개 여부
    profileImageUrl: Img
    role: MemberRole! # enums

    #info: MemberInfo!
    email: String
    nickname: String
    job: String
    category: [Category]
    birthday: Date
    gender: String

    memberActivityInfo: MemberActivityInfo

    challenge: [Challenge]
    diary: [Diary]
}

enum MemberRole {
    USER
    ADMIN
}

